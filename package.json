{
  "name": "ryha-ai-dashboard",
  "version": "1.0.0",
  "description": "Ryha AI Project Management Dashboard - Web Application",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^6.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "sqlite3": "^5.1.6",
    "multer": "^1.4.4",
    "express-rate-limit": "^6.7.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "concurrently": "^7.6.0"
  },
  "keywords": [
    "project-management",
    "dashboard",
    "cybersecurity",
    "ai-agents",
    "react",
    "nodejs"
  ],
  "author": "Velluraju - Ryha AI",
  "license": "MIT"
}